min_peers = 1
max_peers = 65536
download_rate = 0
upload_rate = 0
directory = /home/al/downloads
session = /home/al/.sessions

# check out new *.torrent files once per 5 seconds
schedule = watch_directory,5,5,load.start=/home/al/Downloads/*.torrent

port_range = 6881-6889
port_random = no

trackers.use_udp.set = yes

#session_save = yes
session.save = yes

#scgi_port = localhost:5000

check_hash = no

